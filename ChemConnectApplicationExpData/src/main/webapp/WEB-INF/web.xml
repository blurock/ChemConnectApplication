<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
<context-param>
		<param-name>callback</param-name>
		<param-value>/oauth2callback</param-value>
	</context-param>
	<context-param>
		<param-name>authdirectory</param-name>
		<param-value>.authorization-chemconnect</param-value>
	</context-param>	
	<context-param>
		<param-name>authfilename</param-name>
		<param-value>clientIDs.yaml</param-value>
	</context-param>
	<context-param>
	    <param-name>localhost</param-name>
	    <param-value>http://localhost:8080</param-value>
	</context-param>
	<context-param>
	    <param-name>remotehost</param-name>
	    <param-value>http://blurock-chemconnect.appspot.com</param-value>
	</context-param>
		<!-- Servlets -->
	<servlet>
		<servlet-name>gcsfileupload</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.base.activity.repositoryfile.FileUploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>gcsfileupload</servlet-name>
		<url-pattern>/gcsfileupload</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>expdatautilities</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.expdata.initialize.ExpDataUtilitiesImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>expdatautilities</servlet-name>
		<url-pattern>/chemconnectapplicationexpdata/expdatautilities</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>observationservices</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.expdata.observation.ObservationServicesImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>observationservices</servlet-name>
		<url-pattern>/chemconnectapplicationexpdata/observationservices</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>spreadsheet</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.expdata.observation.spreadsheet.SpreadSheetServicesImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>spreadsheet</servlet-name>
		<url-pattern>/chemconnectapplicationexpdata/spreadsheet</url-pattern>
	</servlet-mapping>
				<servlet>
		<servlet-name>callbackservlet</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.base.authentification.Oauth2CallbackServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>callbackservlet</servlet-name>
		<url-pattern>/oauth2callback</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>loginservice</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.base.authentification.LoginServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginservice</servlet-name>
		<url-pattern>/chemconnectapplicationexpdata/loginservice</url-pattern>
		<url-pattern>/loginservice</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>catalogaccess</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.base.services.BaseCatalogAccessImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>catalogaccess</servlet-name>
		<url-pattern>/chemconnectapplicationexpdata/catalogaccess</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>images</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.base.db.UserImageServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>images</servlet-name>
		<url-pattern>/chemconnectapplicationexpdata/images</url-pattern>
	</servlet-mapping>
			<filter>
		<filter-name>ObjectifyFilter</filter-name>
		<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ObjectifyFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>info.esblurock.reaction.core.server.base.register.RegisterChemConnectBaseCatalogObjects</listener-class>
	</listener>
    <welcome-file-list>
    <welcome-file>ChemConnectApplicationExpData.html</welcome-file>
    <welcome-file>ChemConnectApplicationExpData.jsp</welcome-file>
  </welcome-file-list>
</web-app>