<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
		.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
		margin: 0px
		}
	</ui:style>
	<m:MaterialDialog ui:field="modal" type="DEFAULT"
		inDuration="500" outDuration="500" dismissible="true">
		<m:MaterialDialogContent
			ui:field="modalcontent">
			<m:MaterialPanel>
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<m:MaterialPanel ui:field="choosename" />
					</m:MaterialColumn>
				</m:MaterialRow>
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<m:MaterialTextArea ui:field="description"
							textColor="BLACK" />
					</m:MaterialColumn>
				</m:MaterialRow>
			</m:MaterialPanel>
		</m:MaterialDialogContent>
		<m:MaterialDialogFooter ui:field="footer">
			<m:MaterialLink ui:field="close" text="CLOSE"
				type="FLAT" iconColor="BLACK" textAlign="RIGHT" />
		</m:MaterialDialogFooter>
	</m:MaterialDialog>
</ui:UiBinder> 