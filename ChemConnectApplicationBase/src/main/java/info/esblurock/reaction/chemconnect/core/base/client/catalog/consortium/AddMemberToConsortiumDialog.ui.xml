<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>

	</ui:style>
	<m:MaterialDialog ui:field="dialog" type="DEFAULT"
		dismissible="false" inDuration="500" outDuration="500">
		<m:MaterialDialogContent>
			<m:MaterialRow>
				<m:MaterialTitle ui:field="title" />
			</m:MaterialRow>
			<m:MaterialRow>
				<ma:autocomplete.MaterialAutoComplete
					ui:field="autocomplete" type="TEXT" limit="1"
					placeholder="Username" />
			</m:MaterialRow>

		</m:MaterialDialogContent>
		<m:MaterialDialogFooter>
			<m:MaterialButton ui:field="close"
				text="Close Dialog" type="FLAT" />
		</m:MaterialDialogFooter>
	</m:MaterialDialog>
</ui:UiBinder> 