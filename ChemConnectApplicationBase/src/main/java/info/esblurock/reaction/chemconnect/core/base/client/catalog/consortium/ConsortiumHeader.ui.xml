<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.box {
		background-color: #bdbdbd;
		border: 3px solid #607d8b;
		margin: 0px;
	}
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
	}
	
	</ui:style>
	<m:MaterialPanel styleName="{style.box}">
	<m:MaterialRow margin="0" styleName="{style.rowbox}">
		<m:MaterialColumn grid="s10" >
			<m:MaterialTooltip ui:field="consortiumnametooltip">
				<m:MaterialLink ui:field="consortiumname" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="delete" iconType="DELETE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="save" iconType="SAVE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow margin="0" styleName="{style.box}">
	    <m:MaterialColumn grid="s2">
	        <m:MaterialLink ui:field="consortiamembers" />
	    </m:MaterialColumn>
	    <m:MaterialColumn grid="s9">
	        <m:MaterialPanel ui:field="consortiapanel" />
	    </m:MaterialColumn>
	    		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="add" iconType="ADD"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>
</m:MaterialPanel>
</ui:UiBinder> 
