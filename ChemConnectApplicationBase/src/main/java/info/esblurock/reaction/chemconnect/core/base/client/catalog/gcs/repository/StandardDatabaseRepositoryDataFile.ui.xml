<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
		margin: 0px
	}	
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialRow margin="0" styleName="{style.rowbox}">
			<m:MaterialColumn grid="s10">
			    <m:MaterialTooltip ui:field="fullname">
				<m:MaterialLink ui:field="filename" textColor="BLACK" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialLink ui:field="save" iconType="SAVE"
					iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialLink ui:field="delete" iconType="DELETE"
					iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
			</m:MaterialColumn>
		</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 